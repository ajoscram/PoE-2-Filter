Show #.name shown
	SetFontSize 36

Show #.name hidden
	SetFontSize 18
    MinimapIcon ICON_DISABLED
    PlayEffect None
    PlayAlertSoundPositional None
    DisableDropSound None

Show #.name catchall
	PlayEffect Pink
	SetBorderColor COLOR_BLACK
	SetBackgroundColor COLOR_PINK
	SetTextColor COLOR_BLACK
	PlayAlertSoundPositional SOUND_ERROR